<Page x:Class="TableTopCrucible.Domain.Library.WPF.Pages.AppSettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TableTopCrucible.Domain.Library.WPF.Pages"
      xmlns:pages="clr-namespace:TableTopCrucible.Domain.Library.WPF.Pages"
      xmlns:pageViewModels="clr-namespace:TableTopCrucible.Domain.Library.WPF.PageViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      d:DataContext="{d:DesignInstance Type={x:Type pageViewModels:AppSettingsPageViewModel}, IsDesignTimeCreatable=false}"
      Title="AppSettingsPage"
      Content="{Binding Settings}"/>
