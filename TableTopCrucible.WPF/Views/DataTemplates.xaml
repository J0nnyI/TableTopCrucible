<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:TableTopCrucible.WPF.Views"
                    xmlns:viewModels="clr-namespace:TableTopCrucible.WPF.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:ItemListViewModel}">
        <views:ItemList/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ItemCardViewModel}">
        <views:ItemCard/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:TagEditorViewModel}">
        <views:TagEditor/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ItemEditorViewModel}">
        <views:ItemEditor/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:FileDefinitionViewModel}">
        <views:FileDefinition/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:DirectorySetupViewModel}">
        <views:DirectorySetup/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:DirectorySetupCardViewModel}">
        <views:DirectorySetupCard/>
    </DataTemplate>
</ResourceDictionary>